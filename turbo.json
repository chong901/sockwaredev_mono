{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "grocery-lens#build": {
      "dependsOn": ["^build"],
      "env": ["EDGEDB_SECRET_KEY", "EDGEDB_INSTANCE"]
    }
  }
}
