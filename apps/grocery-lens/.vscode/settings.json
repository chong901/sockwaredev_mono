{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "src/app/api/graphql/route.ts",
        "cmd": "pnpm -F grocery-lens graphql:codegen"
      },
      {
        "match": "src/graphql/(query|mutation).ts",
        "cmd": "pnpm -F grocery-lens graphql:codegen"
      },
      {
        "match": "dbschema/default.esdl",
        "cmd": "pnpm -F grocery-lens db:generate"
      }
    ]
  }
}
