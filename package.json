    "@as-integrations/next": "^3.1.0",
    "@ducanh2912/next-pwa": "^10.2.8",
    "@mapbox/polyline": "^1.2.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
